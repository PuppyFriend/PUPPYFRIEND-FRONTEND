<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root_layout"
    android:background="@android:color/transparent"
    android:layout_marginLeft="79dp"
    android:layout_marginTop="30dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_delete"
        android:layout_width="85.72dp"
        android:layout_height="17.32dp"
        android:text="삭제"
        android:fontFamily="@font/inter_semibold"
        android:textFontWeight="600"
        android:textSize="9sp"
        android:textColor="#D63228"
        android:background="@drawable/style_sns_delete_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_select_delete"
        android:layout_width="85.72dp"
        android:layout_height="17.32dp"
        android:background="@drawable/style_sns_select_delete_btn"
        android:fontFamily="@font/inter_semibold"
        android:text="선택 삭제"
        android:textColor="#D63228"
        android:textFontWeight="600"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_delete"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
