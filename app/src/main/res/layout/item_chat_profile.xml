<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="66.63dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/swipe_itemview"
        android:layout_width="133.26dp"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_alarm"
            android:layout_width="66.63dp"
            android:layout_height="66dp"
            android:background="#C4C4C4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:drawableTop="@drawable/icon_chat_alarm"
            android:paddingTop="21dp"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_delete"
            android:layout_width="66.63dp"
            android:layout_height="66dp"
            android:background="#D63228"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:drawableTop="@drawable/icon_chat_trash"
            android:paddingTop="21dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <!--상단 아이템 view-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#D1D3D9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/img_profile"
            android:layout_width="44dp"
            android:layout_height="44.44dp"
            android:layout_marginStart="32.08dp"
            android:background="@drawable/style_chat_profile"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txt_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="86.06dp"
            android:fontFamily="@font/inter_600"
            android:text="보호자, 퍼피 프로필"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>