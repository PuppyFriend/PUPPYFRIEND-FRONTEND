<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:id="@+id/layout_sns_my_story"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/img_sns_profile"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/img_sns_my_story"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:gravity="center"
            android:background="@drawable/style_home_profile"
            android:scaleType="centerCrop" />

    </LinearLayout>


    <TextView
        android:id="@+id/text_sns_my_story_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/inter_semibold"
        android:lineHeight="10.89dp"
        android:text="ME"
        android:textColor="#565656"
        android:textFontWeight="600"
        android:textSize="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_sns_my_story" />

</androidx.constraintlayout.widget.ConstraintLayout>